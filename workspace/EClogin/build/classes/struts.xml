<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<!-- Strutsの詳細設定 -->
<struts>
	<constant value="true" name="struts-default"/>
	<!-- 対象のpackageを設定 -->
	<package name="com.internousdev.EClogin.action" extends="struts-default">

		<!-- HomeAction -->
		<action name="HomeAction" class="com.internousdev.EClogin.action.HomeAction" method="execute">
			<result name="success">login.jsp</result>
		</action>
	</package>
</struts>